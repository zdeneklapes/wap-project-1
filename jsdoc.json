{
  "source": {
    "include": ["primes.mjs"],
    "includePattern": ".+\\.(js|mjs)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
